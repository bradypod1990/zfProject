zf-v1#springmvc
spring.thymeleaf.prefix=classpath:/views/
spring.thymeleaf.suffix=.html
spring.thymeleaf.cache=false 

spring.application.name=zf-web
#端口号
server.port=30001
#注册中心地址
eureka.client.service-url.defaultZone=http://webone:10001/eureka

#设置Ip和端口号
eureka.instance.instance-id: webone:${server.port}
eureka.instance.prefer-ip-address:true
#主分发器Servlet的路径。（默认值： /。）
server.servlet-path=/
#会话超时时间，单位为秒
server.session.timeout=3000