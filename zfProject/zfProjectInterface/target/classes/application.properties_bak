//必须是host名称，不能是IP地址
spring.data.mongodb.uri=mongodb://com.feng.mongo:27111017/test


#服务名称
spring.application.name=zf-v1
#端口号
server.port=20001
#设置Ip和端口号
eureka.instance.instance-id: webone:${server.port}
eureka.instance.prefer-ip-address:true
#注册中心地址
#eureka.client.service-url.defaultZone=http://webone:10001/eureka/,http://webone:10002/eureka/
eureka.client.service-url.defaultZone=http://webone:10001/eureka/